@{
    Layout = null;
}
<!DOCTYPE html>

<html>
    <head>
        <title>Prova de Desenvolvimento</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">    </head>
        <link href="~/Content/Site.css" rel="stylesheet" />    
<body> 
        <div class="container-fluid mt-5 align-items-center">
            <h2>Cadastro de Usuários</h2>
            <form class="align-content-end">
                <div>
                    <p>Nome:</p>
                    <input id="name" type="text"/>
                </div>
                <div>
                    <p>Sobrenome:</p>
                    <input id="lastName" type="text"/>
                </div>
                <div>
                    <p>Email:</p>
                    <input id="email" type="email"/>
                </div>
                <div>
                    <p>Data de nascimento:</p>
                    <input id="birthDate" type="date" />
                </div>
                <div>
                    <p>Senha:</p>
                    <input id="password" type="password"/>
                </div>
                <div>
                    <p>Confirmação de Senha:</p>
                    <input id="confirmPassword" type="password"/>
                </div>
            </form>
            <button class="button btn btn-dark">Cadastrar</button>
        </div>
    </body>
</html>


<script>
    // Inserir usuário
    function inserirUsuario() {
        jQuery.ajax({
            url: '/UserViewModels/Create',
            type: 'POST',
            data: {
                Id: (Math.floor((Math.random() * 1000) + 1)).toString(),
                FirstName: 'Marcio',
                LastName: 'Laubstein',
                Email: 'marcio.laubstein@totvs.com.br',
                BirthDate: '07/09/1989',
                Password: '123@mudar',
                ConfirmPassword: '123@mudar'
            },
            success: function (result) {
                debugger
            },
            error: function () {
                debugger
            }
        });
    }

    jQuery(document).ready(function(){
        jQuery('button').click(function() { inserirUsuario()})
    }
    // Listar usuários
    jQuery.ajax({
        url: '/UserViewModels/List', 
        type: 'GET', 
        success: function (result) { 
            console.info(JSON.stringify(result)); 
        }, 
        error: function () { 
        } 
    });
</script>